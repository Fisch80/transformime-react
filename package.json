{
  "name": "transformime-react",
  "version": "0.0.0",
  "description": "Transformime that returns React components",
  "files": [
    "*.md",
    "src",
    "lib"
  ],
  "main": "lib/index",
  "jsnext:main": "src/index",
  "scripts": {
    "build": "npm run build:es5",
    "build:es5": "babel src --out-dir lib/ --source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgbkrk/transformime-react.git"
  },
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/rgbkrk/transformime-react/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "parallelshell": "^1.2.0",
    "rimraf": "^2.4.1"
  },
  "homepage": "https://github.com/rgbkrk/transformime-react#readme",
  "dependencies": {
    "immutable": "^3.7.6",
    "react": "^0.14.5"
  }
}
